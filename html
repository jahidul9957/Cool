		<script type="text/javascript">
		
		    let img = new Image();
		    var coords = '';
		    var txcoords = '';
		    img.src = '/static/img/rechange.svg'
		    var y2 = window.innerWidth*0.9*0.5;
		    var x = y2;
		    var y = y2 + window.innerWidth*0.9*0.5;
		    
		    coords = '0,0,'+y2+','+img.height;
		    txcoords = x+','+img.height+','+y+',0';//:coords="140,35,260,0"
		    var map = document.getElementsByClassName('recharge-btn')[0];
		    map.setAttribute("coords", coords);

		    var txmap = document.getElementsByClassName('withdraw-btn')[0];
		    txmap.setAttribute("coords", txcoords);
		    console.log(window.innerWidth,coords,txcoords)
		    
		    
		    var box = document.getElementById('notice-box')
            var parent = document.getElementsByTagName('ul')[0]
            var parent2 = document.getElementsByTagName('ul')[1]
            parent2.innerHTML = parent.innerHTML

            /*启动定时器*/
            var timer = setInterval(autoScrollLine,100)
            /*单行向上滚动效果*/
            function autoScrollLine() {
                /*判断滚动内容是否已经滚完，滚完了则滚动的值重新设置到0 否则就每隔30毫秒向上滚动1px*/
                if(box.scrollTop >= parent.offsetHeight){
                    box.scrollTop=0;
                }else{
                    box.scrollTop++;
                }
                
                /*判断滚动的距离刚好为一条公告的高度时停掉定时器，隔1s之后重新启动定时器即可实现公告滚动停留效果 */
                if( (box.scrollTop>29 && box.scrollTop<31) || (box.scrollTop>89 && box.scrollTop<91)){
                    if(parseInt(box.scrollTop)%box.offsetHeight==0){
                        clearInterval(timer)
                        
                        setTimeout(()=>{
                        
                            timer = setInterval(autoScrollLine,100)
                        
                        },3000)
                    }
                }else{
                    if(box.scrollTop%box.offsetHeight==0){
                        clearInterval(timer)
                        
                        setTimeout(()=>{
                        
                            timer = setInterval(autoScrollLine,100)
                        
                        },3000)
                    }
                }
            }
   
		
		    $(document).ready(function(){

                $(".btn-close,.maskLayer").on("click",function(e){
                
            	    // 点击关闭弹窗按钮及遮罩层关闭弹窗事件
            	    $(".maskLayer,.popup").hide();
            	    // 解除禁止滑动
            		$("body").css("overflow","");
            	})
            });
		    
			(function($) {
				if ($.fn.owlCarousel) {
					var welcomeSlider = $('.hero-slides');
					welcomeSlider.owlCarousel({
						items: 1,
						loop: true,
						autoplay: true,
						dots: false,
						margin: 0,
						nav: false,
						smartSpeed: 1000,
						autoplayTimeout: 5000
					})
				}

			})(jQuery);


			
			var isview=0;
					
			if(isview==1){
				 $("#gui_kai").show();
			}

			$('#g_close').click(function() {
				$("#gui_kai").hide();
			});
		</script>
		
